<html>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.min.css">
  <head>
    <title>A-Wiki</title>
    <meta property="og:title" content="Aerial Laptop">
    <meta property="og:site_name" content="">
    <meta property="og:url" content="">
    <meta property="og:description" content="Welcome to the Aerial Laptop homepage!">
    <meta property="og:type" content="article">
    <meta property="og:image" content="">
  </head>
  <body>
    <h1 class="text-center">We are <b>Aerial Laptop!</b>.</h1>
    <div id="break">
      <img class="text-center" src="https://i.imgur.com/xglYTRY.png" alt="Logo" width="250" height="250">
    </div>
    <div id="wiki">
      <div class="jumbotron">
        <b>You can find information on our projects below!</b>
        <hr>
        <b>Current</b>
        <% for (post of current) { %>
        <a href="/<%= post.slice(0, -3) %>">
          <div class="post">
            <%= post.replace(/.md|-|(current-)/g, ' '); %>
          </div>
        </a>
        <% } %>
        <b>Legacy</b>
        <% for (post of legacy) { %>
        <a href="/<%= post.slice(0, -3) %>">
          <div class="post">
            <%= post.replace(/.md|-|(legacy-)/g, ' '); %>
          </div>
        </a>
        <% } %>
    </div>
    </div>
    <footer>
      <p class="text-center text-muted">Contact us at aeriallaptop(a)enbyte.dev</p>
      <p class="text-center text-muted">&copy; <b>Aerial Laptop 2022</b></p>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/vanta@latest/dist/vanta.topology.min.js"></script>
    <script>
      VANTA.TOPOLOGY({
        el: "#break",
        mouseControls: true,
        touchControls: true,
        gyroControls: false,
        minHeight: 200.00,
        minWidth: 200.00,
        scale: 1.00,
        scaleMobile: 1.00,
        color: 0x0,
        backgroundColor: 0xFFFFFF
      })
    </script>
  </body>
</html>
